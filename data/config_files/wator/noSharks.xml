<?xml version="1.0" encoding="UTF-8" ?>
<simulation type="wator">
  <title>WA-Tor</title>
  <author>John Horton Conway</author>
  <desc>Simulation designed to show the interaction of species</desc>

  <coreSpecifications>
    <type cellPackage ="wator"></type>
    <type gridType = "ToroidalGrid"></type>
    <type stepperType = "WatorStepper"></type>
    <type structureType = "Square"></type>
  </coreSpecifications>

  <gridInfo>
    <rows>10</rows>
    <columns>10</columns>

    <grid>
      <type>Grid</type>
      <gridState>
        eeefefeeee
        eeeeeeeeee
        eeefeeeeee
        eeeeeeeeee
        feeefeeeee
        feeeeeeefe
        eeeeeeeeef
        eeeeefeeef
        eeeeeeeeee
        eeeeeeeeee
      </gridState>

      <randomProbs>
      </randomProbs>

      <codes>
        <key f='FishCell'></key>
        <key s='SharkCell'></key>
        <key e ='Empty'></key>
      </codes>

      <decoder>
        <key FishCell = '1'></key>
        <key SharkCell = '2'></key>
      </decoder>

      <parameters>
        <parameter breedtimecounter='5'/>
        <parameter breedtimethresh='0'/>
        <parameter energy='10'/>
        <parameter energygain = '10'/>
        <parameter reproducethresh='15'/>
      </parameters>
    </grid>

  </gridInfo>

</simulation>