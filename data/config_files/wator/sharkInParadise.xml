  <colors>
    <key f = 'CCAA00'></key>
    <key s = 'AAAAAA'></key>
    <key e = '0000FF'></key>
  </colors>

  <parameters>
    <parameter breedtimecounter='0'/>
    <parameter breedtimethresh='10'/>
    <parameter energy='10'/>
    <parameter energygain = '3'/>
    <parameter reproducethresh='15'/>
  </parameters>

</simulation>