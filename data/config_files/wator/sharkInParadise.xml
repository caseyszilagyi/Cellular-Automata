<?xml version="1.0" encoding="UTF-8" ?>
<simulation type="wator">
  <title>WA-Tor</title>
  <author>John Horton Conway</author>
  <desc>Simulation designed to show the interaction of species</desc>

  <coreSpecifications>
    <type cellPackage ="wator"></type>
    <type gridType = "ToroidalGrid"></type>
    <type stepperType = "WatorStepper"></type>
    <type structureType = "Square"></type>
  </coreSpecifications>

  <gridInfo>
    <rows>4</rows>
    <columns>10</columns>

    <grid>
      <type>Grid</type>
      <gridState>
        fffeeeefff
        fsfffeffff
        ffffefffsf
        fffeeeefff
      </gridState>

      <codes>
        <key f='FishCell'></key>
        <key s='SharkCell'></key>
        <key e ='Empty'></key>
      </codes>

      <decoder>
        <key FishCell = '1'></key>
        <key SharkCell = '2'></key>
      </decoder>

      <parameters>
        <parameter breedtimecounter='0'/>
        <parameter breedtimethresh='10'/>
        <parameter energy='10'/>
        <parameter energygain = '3'/>
        <parameter reproducethresh='15'/>
      </parameters>
    </grid>

  </gridInfo>

  <colors>
    <key f = 'CCAA00'></key>
    <key s = 'AAAAAA'></key>
    <key e = '0000FF'></key>
  </colors>

</simulation>