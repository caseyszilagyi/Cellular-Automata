<?xml version="1.0" encoding="UTF-8" ?>
<simulation type="wator">
  <cellPackage>wator</cellPackage>
  <title>WA-Tor</title>
  <author></author>
  <desc>Simulation designed to show the interaction of species</desc>

  <cellPackage>wator</cellPackage>
  <gridType>ToroidalGrid</gridType>
  <stepperType>WatorStepper</stepperType>

  <rows>4</rows>
  <columns>10</columns>
  <grid>
    fffeeeefff
    fsfffeffff
    ffffefffsf
    fffeeeefff
  </grid>

  <codes>
    <key f='FishCell'></key>
    <key s='SharkCell'></key>
