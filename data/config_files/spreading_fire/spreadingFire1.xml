<?xml version="1.0" encoding="UTF-8" ?>
<simulation type="Spreading Fire">
  <cellPackage>spreading_fire</cellPackage>
  <title>Spreading Fire</title>
  <author></author>
  <desc>Simulation designed to show the spread of wildfire in a forest</desc>
  <rows>4</rows>
  <columns>4</columns>
  <grid>
    t t t e
    f t e e
    f t t t
    t t t e
  </grid>

  <codes>
    <key t='TreeCell'></key>
    <key f='FireCell'></key>
    <key e ='Empty'></key>
  </codes>

  <parameters>
    <parameter probcatch='0.5'/>
  </parameters>

</simulation>