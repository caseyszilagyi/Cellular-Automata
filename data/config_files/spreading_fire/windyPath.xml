<?xml version="1.0" encoding="UTF-8" ?>
<simulation type="spreading_fire">
  <title>Spreading Fire</title>
  <author></author>
  <desc>Simulation designed to show the spread of wildfire in a forest</desc>

  <coreSpecifications>
    <type cellPackage ="spreading_fire"></type>
    <type gridType = "Grid"></type>
    <type stepperType = "SimulationStepper"></type>
    <type structureType = "Square"></type>
  </coreSpecifications>

  <gridInfo>
    <rows>15</rows>
    <columns>10</columns>

    <grid>
      <type>Grid</type>
      <gridState>
        fttttttttt
        eeeeeeeeet
        tttttttttt
        teeeeeeeee
        tttttttttt
        eeeeeeeeet
        tttttttttt
        teeeeeeeee
        tttttttttt
        eeeeeeeeet
        eeeeeeeett
        eeeeeeettt
        eeeeeetttt
        eeeeettttt
        eeeetttttt
      </gridState>

      <randomProbs>
      </randomProbs>

      <codes>
        <key t='TreeCell'></key>
        <key f='FireCell'></key>
        <key e ='Empty'></key>
      </codes>

      <decoder>
        <key TreeCell = '1'></key>
        <key FireCell = '2'></key>
      </decoder>

      <parameters>
        <parameter probcatch='1'/>
      </parameters>
    </grid>
  </gridInfo>

</simulation>